<!DOCTYPE html>
<html>
<head>
    <title>@ViewBag.Title</title>
    <script src="@Url.Content("~/Scripts/jquery/jquery-1.6.2.min.js")" type="text/javascript"></script>
    <script src="@Url.Content("~/Scripts/jquery-ui/jquery-ui-1.8.15.custom.min.js")" type="text/javascript"></script>
    <script src="@Url.Content("~/Scripts/jquery/jquery.blockUI.js")" type="text/javascript"></script>
    <script type="text/javascript" src="@Url.Content("~/Scripts/fancybox/fancybox/jquery.mousewheel-3.0.2.pack.js")"></script>
    <script type="text/javascript" src="@Url.Content("~/Scripts/fancybox/fancybox/jquery.fancybox-1.3.1.js")"></script>
    <link rel="stylesheet" type="text/css" href="@Url.Content("~/Scripts/fancybox/fancybox/jquery.fancybox-1.3.1.css")"
        media="screen" />
    <!-- <script src="@Url.Content("~/Scripts/jquery.fancybox/jquery.fancybox-1.3.4.js")"  type="text/javascript"></script>
        <link href="@Url.Content("~/Scripts/jquery.fancybox/themes/jquery.fancybox-1.3.4.css")" rel="stylesheet" type="text/css" />-->
    <link href="@Url.Content("~/Scripts/jquery-ui/theme/jquery-ui-1.8.15.custom.css")" rel="stylesheet"
        type="text/css" />
    <script type="text/javascript" src="@Url.Content("~/Scripts/ckeditor/ckeditor.js")"></script>
    <script type="text/javascript" src="@Url.Content("~/Scripts/ckeditor/adapters/jquery.js")"></script>
    <script src="@Url.Content("~/Scripts/menuLateral.js")" type="text/javascript"></script>
    <!-- ESTRUTURA DE FORMULARIOS -->
    <script src="@Url.Content("~/Scripts/jquery.uniform/jquery.uniform.min.js")" type="text/javascript"></script>
    <link href="@Url.Content("~/Scripts/jquery.uniform/css/uniform.default.css")" rel="stylesheet" type="text/css" />
    <script src="@Url.Content("~/Scripts/jquery/jquery.maskedinput-1.3.min.js")" type="text/javascript"></script>
    <script src="@Url.Content("~/Scripts/jquery.validate/jquery.validate.min.js")" type="text/javascript"></script>
    <script src="@Url.Content("~/Scripts/jquery.validate/additional-methods.js")" type="text/javascript"></script>
    <script src="@Url.Content("~/Scripts/jquery.validate/messages_ptbr.js")" type="text/javascript"></script>
    <script src="@Url.Content("~/Scripts/formObj.js")" type="text/javascript"></script>
    <link href="@Url.Content("~/Scripts/jquery-ui/theme/jquery.ui.selectmenu.css")" rel="stylesheet" type="text/css" />
    <script src="@Url.Content("~/Scripts/jquery-ui/jquery.ui.selectmenu.js")" type="text/javascript"></script>
    <link href="@Url.Content("~/Scripts/jquery.datepick/jquery.datepick.css")" rel="stylesheet" type="text/css" />
    <link href="@Url.Content("~/Scripts/jquery.datepick/redmond.datepick.css")" rel="stylesheet" type="text/css" />
    <script src="@Url.Content("~/Scripts/jquery.datepick/jquery.datepick.min.js")" type="text/javascript"></script>
    <script src="@Url.Content("~/Scripts/jquery.datepick/jquery.datepick-pt-BR.js")" type="text/javascript"></script>
    <!-- FIM ESTRUTURA DE FORMULARIOS -->
    <!-- CSS COM A ESTRUTURA DO SITE -->
    <link href="@Url.Content("~/Content/SiteEstrutura.css")" rel="stylesheet" type="text/css" />
    <link href="@Url.Content("~/Content/SiteEstrutura_GIS.css")" rel="stylesheet" type="text/css" />
    <link href="@Url.Content("~/Content/FormEstrutura.css")" rel="stylesheet" type="text/css" />
    <!-- FIM CSS COM A ESTRUTURA DO SITE -->
</head>
<body>
    <!-- TOPO -->
    <div class="estruturaTopo">
        <div class="div956">
            <div class="bgTopo">
                <div class="wrapTituloTopo">
                    <span>BurBs Na Balada </span>
                </div>
            </div>
        </div>
    </div>
    <!-- IMG LOADING -->
    <div id="loading" style="display: none">
        <div>
            <table border="0" width="100%" height="100%">
                <tr>
                    <td valign="middle">
                        <img alt="" src="@Url.Content("~/Content/Images/bigLoader.gif")" /><br />
                        <br />
                        Carregando
                    </td>
                </tr>
            </table>
        </div>
    </div>
    <!-- IMG LOADING -->
    <div class="paginaLogin_Loader" style="display: none;">
        <img src="@Url.Content("~/Content/Images/Loader.gif")" />
    </div>
    <!-- CORPO -->
    <div class="estruturaCorpo">
        @{
            if (!Request.RawUrl.ToUpper().Contains("LOGIN"))
            {
            <div class="div956">
                <!-- MENU ESQUERDO -->
                <div class="corpoLeft">
                    <div class="menuLeftTopo">
                        <div class="menuLeftTopo_wrapLogOff">
                            <form id="logoff" name="logoff" action="Login/LogOff" method="post">
                            <a onclick="document.logoff.submit();"><span>LogOff</span></a>
                            </form>
                        </div>
                        <div class="menuLeftTopo_wrapNomeUser">
                            <div>
                                @Session["USUARIO"].ToString().ToUpper()
                                <br />
                                @Session["EMAIL"].ToString().ToUpper()
                            </div>
                        </div>
                        <div class="cb">
                            &nbsp;</div>
                    </div>
                    <div class="menuLeft_wrapNavegacao">
                        <div class="menuPrincipalTopo">
                        </div>
                        <!-- HEADER -->
                        <ul id="menuLateral" class="menuPrincipalListagem">
                            <!--MenuLateral-->
                            <li id="liPaginaInicial"><a id="linkPaginaInicial" onclick="carregaConteudo('Pagina_Inicial.php')">
                                <img id="imgPaginaInicial" src="@Url.Content("~/Content/Images/Icons/icoPaginaInicial.png")" />P&aacute;gina
                                Inicial</a> </li>
                            <li id="liCadastro"><a id="linkCadastro">
                                <img id="imgCadastro" src="@Url.Content("~/Content/Images/Icons/icoPesquisa.png")" />Pesquisa</a>
                                <ul id="wrapSubmenuCadastro" visible="false">
                                    <li><a id='linkSubmenuLaboratoriosPesquisa' onclick="efeitoLoading(carregaConteudo('Pesquisa/LaboratoriosPesquisa/LaboratoriosPesquisa_Listagem.php'))">
                                        Laboratorios de Pesquisa</a></li>
                                    <li><a id='linkSubmenuLinhasPesquisa' onclick="efeitoLoading(carregaConteudo('Pesquisa/LinhasPesquisa/LinhasPesquisa_Listagem.php'))">
                                        Linhas de Pesquisa</a></li>
                                </ul>
                            </li>
                            <li id="liCadastro"><a id="linkCadastro">
                                <img ID="imgCadastro" src="@Url.Content("~/Content/Images/Icons/icoCadastro.png")"/>Cadastro
                            </a>
                                <ul id="wrapSubmenuCadastro" visible="false">
                                    <li><a id='linkSubmenuMenu' onclick="efeitoLoading(carregaConteudo('Cadastro/Menu/Menu_Listagem.php'))">
                                        N&iacute;vel 1 - Menu</a></li>
                                    <li><a id='linkSubmenuGrupo' onclick="efeitoLoading(carregaConteudo('Cadastro/Grupo/Grupo_Listagem.php'))">
                                        N&iacute;vel 2 - Grupo</a></li>
                                    <li><a id='linkSubmenuSubGrupo' onclick="efeitoLoading(carregaConteudo('Cadastro/SubGrupo/SubGrupo_Listagem.php'))">
                                        N&iacute;vel 3 - SubGrupo</a></li>
                                    <li><a id='linkSubmenuItens' onclick="efeitoLoading(carregaConteudo('Cadastro/Itens/Itens_Listagem.php'))">
                                        N&iacute;vel 4 - Itens</a></li>
                                    <li><a id='linkSubmenuItens' onclick="efeitoLoading(carregaConteudo('Cadastro/Area/Area_Listagem.php'))">
                                        Contato</a></li>
                                </ul>
                            </li>
                            <li id="liCanais"><a id='linkCanais' onclick="efeitoLoading(carregaConteudo('Canais/Canais_Listagem.php'))">
                                <img id="imgCanais" src="@Url.Content("~/Content/Images/Icons/icoCanais.png")" />Canais</a>
                            </li>
                            <li id="liConfiguracoes"><a id="linkConfiguracoes">
                                <img id="imgConfiguracoes" src="@Url.Content("~/Content/Images/Icons/icoConfiguracao.png")" />Configura&ccedil;&otilde;es</a>
                                <ul id="wrapSubmenuConfiguracoes" visible="false">
                                    <li><a id='linkSubmenuUsuarios' onclick="efeitoLoading(carregaConteudo('Configuracoes/Usuarios/Usuarios_Listagem.php'));">
                                        Usu&aacute;rios</a></li>
                                    <li><a id='linkSubmenuBanners' onclick="efeitoLoading(carregaConteudo('Configuracoes/Banners/Banner_Listagem.php'))">
                                        Banners</a></li>
                                    <li><a id='linkSubmenuPermissoes' onclick="efeitoLoading(carregaConteudo('Configuracoes/Permissoes/Permissoes_Listagem.php'))">
                                        Permiss&otilde;es</a></li>
                                </ul>
                            </li>
                            <li id="liConteudo"><a id='linkConteudo' onclick="efeitoLoading(carregaConteudo('Conteudos/Conteudos_Listagem.php'))">
                                <img id="imgConteudo" src="@Url.Content("~/Content/Images/Icons/icoConteudo.png")" />Conte&uacute;dos</a>
                            </li>
                            <li id="liCursos"><a id="linkCursos" onclick="efeitoLoading(carregaConteudo('Cursos/Cursos_Listagem.php'))">
                                <img id="imgCursos" src="@Url.Content("~/Content/Images/Icons/icoEnsino.png")" />Cursos</a>
                            </li>
                            <li id="liPaginaPessoal"><a id='linkPaginaPessoal' onclick="efeitoLoading(carregaConteudo('PaginaPessoal/PaginaPessoal_Listagem.php'))">
                                <img id="imgPaginaPessoal" src="@Url.Content("~/Content/Images/Icons/icoPaginaPessoal.png")" />Pagina
                                Pessoal</a> </li>
                            <li id="liProjetos"><a id='linkProjetos' onclick="efeitoLoading(carregaConteudo('Projetos/Projetos_Listagem.php'))">
                                <img id="imgProjetos" src="@Url.Content("~/Content/Images/Icons/icone-cultura.png")"/>Projetos
                                Culturais</a> </li>
                            <li id="liSistemas"><a id='linkSistemas' onclick="efeitoLoading(carregaConteudo('Sistemas/Sistemas_Listagem.php'))">
                                <img id="imgSistemas" src="@Url.Content("~/Content/Images/Icons/icoSistemas.png")" />Sistemas</a>
                            </li>
                        </ul>
                        <div class="menuPrincipalRodape">
                        </div>
                    </div>
                </div>
                <!-- MIOLO DIREITO-->
                <div class="corpoRight">@RenderBody()<!-- DIV CARREGA OS CONTEUDOS DAS PAGINAS --></div>
            </div>
            }
            else
            {
            @RenderBody()
            }
        }
    </div>
    <!-- RODAPÉ-->
    <div class="estruturaRodape">
        <div class="div956">
            <div class="bgRodape">
            </div>
        </div>
    </div>
    <script type="text/javascript">        objForm.Init(); </script>
</body>
</html>